		rgltr-load-current = <74000 103000 103000 0>;
		gpio-no-mux = <0>;
		cci-master = <0>;
		status = "ok";
		clocks = <&clock_camcc CAM_CC_MCLK0_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "nominal";
		clock-rates = <19200000>;
	};

	/* back main sensor */
	qcom,cam-sensor0 {
		cell-index = <4>;
		compatible = "qcom,cam-sensor";
		csiphy-sd-index = <1>;
		sensor-position-roll = <90>;
		sensor-position-pitch = <0>;
		sensor-position-yaw = <180>;
		eeprom-src = <&eeprom_rear_main>;
		actuator-src = <&actuator_triple_main>;
		led-flash-src = <&led_flash_triple_rear>;
		ois-src  = <&ois0>;
		cam_vio-supply = <&pm8008i_l5>;
		cam_vana-supply = <&pm8008i_l7>;
		cam_vdig-supply = <&pm8008i_l4>;
		cam_vaf-supply = <&pm8008i_l3>;
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		regulator-names = "cam_vio", "cam_vana", "cam_vdig", "cam_vaf", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 2850000 3140000 3200000 0>;
		rgltr-max-voltage = <1900000 2950000 3200000 3200000 0>;
		rgltr-load-current = <74000 103000 103000 103000 0>;
