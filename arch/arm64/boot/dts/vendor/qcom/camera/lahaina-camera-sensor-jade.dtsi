/* laser */
&qupv3_se0_i2c {
	status = "ok";

	tof8801: tof8801@41 {
		cell-index = <0>;
		compatible = "huawei,laser";
		reg = <0x41>;
		vdd_ana-supply = <&pm8350_l7>;
		gpios = <&tlmm 120 0>,
			<&tlmm 119 0>;
		pinctrl-names = "laser_int_active", "laser_en_active";
		pinctrl-0 = <&laser_int_active>;
		pinctrl-1 = <&laser_en_active>;
		gpio-req-tbl-num = <0>;
		gpio-req-tbl-flags = <0>;
		gpio-req-tbl-label = "CAM_VAF0";
		status = "ok";
	};

	vl53l1: vl53l1@29 {
		cell-index = <1>;
		compatible = "huawei,laser";
		reg = <0x29>;
		vdd_ana-supply = <&pm8350_l7>;
		gpios = <&tlmm 120 0>,
			<&tlmm 119 0>;
		pinctrl-names = "laser_int_active", "laser_en_active";
		pinctrl-0 = <&laser_int_active>;
		pinctrl-1 = <&laser_en_active>;
		gpio-req-tbl-num = <0>;
		gpio-req-tbl-flags = <0>;
		gpio-req-tbl-label = "CAM_VAF0";
		status = "ok";
	};

	vi5300: vi5300@6c {
		cell-index = <2>;
		compatible = "huawei,laser";
		reg = <0x6c>;
		vdd_ana-supply = <&pm8350_l7>;
		gpios = <&tlmm 120 0>,
			<&tlmm 119 0>;
		pinctrl-names = "laser_int_active", "laser_en_active";
		pinctrl-0 = <&laser_int_active>;
		pinctrl-1 = <&laser_en_active>;
		gpio-req-tbl-num = <0>;
		gpio-req-tbl-flags = <0>;
		gpio-req-tbl-label = "CAM_VAF0";
		status = "ok";
	};
};

&cam_cci0 {
	ois0: qcom,ois0 {
		cell-index = <4>;
		compatible = "qcom,ois";
		cci-master = <1>;
		cam_vio-supply = <&pm8008i_l5>;
		cam_vdig-supply = <&pm8008i_l4>;
		regulator-names = "cam_vio", "cam_vdig";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 3140000>;
		rgltr-max-voltage = <1900000 3200000>;
		rgltr-load-current = <74000 103000>;
	};

	ois1: qcom,ois1 {
		cell-index = <5>;
		compatible = "qcom,ois";
		cci-master = <0>;
		cam_vio-supply = <&pm8008i_l5>;
		cam_vdig-supply = <&pm8008i_l4>;
		cam_vaf-supply = <&pm8008i_l3>;
		regulator-names = "cam_vio", "cam_vdig", "cam_vaf";
		rgltr-cntrl-support;
		rgltr-min-voltage = <1800000 3140000 3200000>;
		rgltr-max-voltage = <1900000 3200000 3200000>;
		rgltr-load-current = <74000 103000 103000>;
	};

	actuator_triple_main: qcom,actuator0 {
		cell-index = <4>;
		compatible = "qcom,actuator";
		cci-master = <1>;
		cam_vaf-supply = <&pm8008i_l3>;
		regulator-names = "cam_vaf";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3200000>;
		rgltr-max-voltage = <3200000>;
		rgltr-load-current = <103000>;
	};

	actuator_triple_sub: qcom,actuator3 {
		cell-index = <6>;
		compatible = "qcom,actuator";
		cci-master = <0>;
		cam_vaf-supply = <&pm8008i_l3>;
		regulator-names = "cam_vaf";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3200000>;
		rgltr-max-voltage = <3200000>;
		rgltr-load-current = <103000>;
