			vddio_gpio_type = <1>;
			vddio_regulator_type = <0>;
			vddio_value = <1800000>;
			vddio_ctrl_gpio = <&tlmm 78 0x00>;
			glove_supported = <0>;

			slave_address = <0x20>;
			irq_config = <3>;
			algo_id = <1>;
			ic_type = <0>;
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&qupv3_se1_i2c_active>;
			pinctrl-1 = <&qupv3_se1_i2c_sleep>;
			x_max = <340>;
			y_max = <340>;
			x_max_mt = <340>;
			y_max_mt = <340>;
			boot-hw-reset-timing = <5 5 50>;
			max_finger_objects = <2>;
			rawdata_newformatflag = <1>;
			roi_supported = <0>;
			roi_control_addr = <0>;
			glove_switch_addr = <0>;
			holster_supported = <0>;
			charger_supported = <0>;
			aft_wxy_enable = <0>;
