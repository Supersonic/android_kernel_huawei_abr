#include "yupik-camera-sensor-rena.dtsi"

&soc {
	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		gpios-shared-pinctrl = <340 341 466 315 403>;
		shared-pctrl-gpio-names = "rear_mcam_afvdd", "mcam_iovdd", "mcam_dvdd", "mcam_avdd", "cam_sensor_mclk3";
		pinctrl-names = "rear_mcam_afvdd_active", "rear_mcam_afvdd_suspend",
				"mcam_iovdd_active", "mcam_iovdd_suspend",
				"mcam_dvdd_active", "mcam_dvdd_suspend",
				"mcam_avdd_active", "mcam_avdd_suspend",
				"cam_sensor_mclk3_active", "cam_sensor_mclk3_suspend";
		pinctrl-0 = <&rear_mcam_afvdd_active>;
		pinctrl-1 = <&rear_mcam_afvdd_suspend>;
		pinctrl-2 = <&mcam_iovdd_active>;
		pinctrl-3 = <&mcam_iovdd_suspend>;
		pinctrl-4 = <&mcam_dvdd_active>;
		pinctrl-5 = <&mcam_dvdd_suspend>;
		pinctrl-6 = <&mcam_avdd_active>;
		pinctrl-7 = <&mcam_avdd_suspend>;
		pinctrl-8 = <&cam_sensor_mclk3_active>;
		pinctrl-9 = <&cam_sensor_mclk3_suspend>;
		status = "ok";
	};
};

&qupv3_se1_i2c {
	status = "disabled";
};

&pm8350c_gpios {
	mcam_avdd_active: mcam_avdd_active {
		pins = "gpio3";
		function = "normal";
		bias-disable;
		output-high;
		power-source = <0>;
	};
	mcam_avdd_suspend: mcam_avdd_suspend {
		pins = "gpio3";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <0>;
	};
};

&tlmm {
	rear_mcam_avdd_active: rear_mcam_avdd_active {
		/* rear_mcam_avdd */
		mux {
			pins = "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio25";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	rear_mcam_avdd_suspend: rear_mcam_avdd_suspend {
		/* rear_mcam_avdd */
		mux {
			pins = "gpio25";
			function = "gpio";
		};

		config {
			pins = "gpio25";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	rear_mcam_afvdd_active: rear_mcam_afvdd_active {
		/* rear_mcam_afvdd */
		mux {
			pins = "gpio4";
			function = "gpio";
		};

		config {
			pins = "gpio4";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	rear_mcam_afvdd_suspend: rear_mcam_afvdd_suspend {
		/* rear_mcam_afvdd */
		mux {
			pins = "gpio4";
			function = "gpio";
		};

		config {
			pins = "gpio4";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	mcam_iovdd_active: mcam_iovdd_active {
		/* mcam_iovdd */
		mux {
			pins = "gpio5";
			function = "gpio";
		};

		config {
			pins = "gpio5";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	mcam_iovdd_suspend: mcam_iovdd_suspend {
		/* mcam_iovdd */
		mux {
			pins = "gpio5";
			function = "gpio";
		};

		config {
			pins = "gpio5";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	mcam_dvdd_active: mcam_dvdd_active {
		/* mcam_dvdd */
		mux {
			pins = "gpio130";
			function = "gpio";
		};

		config {
			pins = "gpio130";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	mcam_dvdd_suspend: mcam_dvdd_suspend {
		/* mcam_dvdd */
		mux {
			pins = "gpio130";
			function = "gpio";
		};

		config {
			pins = "gpio130";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};
};

