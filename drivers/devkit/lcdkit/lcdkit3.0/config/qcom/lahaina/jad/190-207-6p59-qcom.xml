		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0
				5, 0, 1
				1, 0, 1
				9, 0, 1
				3, 0, 1
				2, 0, 1"</PowerOffStage>
