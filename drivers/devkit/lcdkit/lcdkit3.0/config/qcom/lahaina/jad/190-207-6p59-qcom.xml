		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 1
				9, 1, 1
				3, 1, 1
				1, 1, 10
				5, 1, 10
				7, 1, 0
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>

