<hwlcd>
	<PanelEntry>
		<!--Panel type:dsi_video_mode-video mode, dsi_cmd_mode-command mode-->
		<PanelTypeMode>"dsi_cmd_mode"</PanelTypeMode>
		<PanelModeSwitch>0</PanelModeSwitch>
		<!--active area size-->
		<PanelPhyWidth>68</PanelPhyWidth>
		<PanelPhyHeight>160</PanelPhyHeight>
		<!--Dsi ctrl number:1-one dsi ctrl, 2-two dsi ctrl-->
		<DsiCtrlNum>0</DsiCtrlNum>
		<!--Dsi phy number:1-one dsi phy, 2-two dsi phy-->
		<DsiPhyNum>0</DsiPhyNum>

		<!-- Lane Config -->
		<PanelDSILaneMap>"lane_map_0123"</PanelDSILaneMap>
		<PanelLane0State>1</PanelLane0State>
		<PanelLane1State>1</PanelLane1State>
		<PanelLane2State>1</PanelLane2State>
		<PanelLane3State>1</PanelLane3State>

		<!-- MIPI Host Config -->
		<DsiSelectClocks>"mux_byte_clk0", "mux_pixel_clk0"</DsiSelectClocks>

		<!-- Display dsi timming config -->
		<DisplayDsiTimmingBegin>1</DisplayDsiTimmingBegin>
		<DisplayDsiTimming0Begin>1</DisplayDsiTimming0Begin>
		<DisplayDsiTimming0End>1</DisplayDsiTimming0End>
		<DisplayDsiTimmingEnd>1</DisplayDsiTimmingEnd>

		<!-- Display command panel config -->
		<PanelTEPinSelect>1</PanelTEPinSelect>
		<PanelWRMEMStart>0x2c</PanelWRMEMStart>
		<PanelWRMEMContinue>0x3c</PanelWRMEMContinue>
		<PanelTEDCSCmd>1</PanelTEDCSCmd>
		<PanelTECheckEnable>1</PanelTECheckEnable>
		<PanelTEUsingTEPin>1</PanelTEUsingTEPin>
		<PanelGpioAddress>0x0F100000 0x1000</PanelGpioAddress>
		<PanelGpioPins>12 24 82</PanelGpioPins>
		<PanelGpioNames>"reg-gpio", "reset-gpio", "platform-te-gpio"</PanelGpioNames>

		<!-- Display dsi timming0 config -->
		<PanelXres0>1188</PanelXres0>
		<PanelYres0>2790</PanelYres0>
		<HFrontPorch0>32</HFrontPorch0>
		<HBackPorch0>32</HBackPorch0>
		<HPulseWidth0>32</HPulseWidth0>
		<VFrontPorch0>16</VFrontPorch0>
		<VBackPorch0>12</VBackPorch0>
		<VPulseWidth0>4</VPulseWidth0>
		<HSyncSkew0>0</HSyncSkew0>
		<PanelFrameRate0>60</PanelFrameRate0>
		<PanelPhyTimings0>"00 13 04 04 13 1E 05
			05 05 04 02 04 10 09"</PanelPhyTimings0>
		<DisplayTopology0>1 1 1</DisplayTopology0>
		<DefaultTopologyIndex0>0</DefaultTopologyIndex0>
		<PanelCompressionMode0>"dsc"</PanelCompressionMode0>
		<PanelDscSliceHeight0>30</PanelDscSliceHeight0>
		<PanelDscSliceWidth0>1188</PanelDscSliceWidth0>
		<PanelDscSlicePerPkt0>1</PanelDscSlicePerPkt0>
		<PanelDscBitPerComponent0>8</PanelDscBitPerComponent0>
		<PanelDscBitPerPixel0>8</PanelDscBitPerPixel0>
		<PanelDscBlockPredictionEnable0>1</PanelDscBlockPredictionEnable0>

		<!-- Backlight ctrl config -->
		<BlPmicControlType>"bl_ctrl_dcs"</BlPmicControlType>
		<MdssDsiBlMinLevel>1</MdssDsiBlMinLevel>
		<MdssDsiBlMaxLevel>4095</MdssDsiBlMaxLevel>
		<DsiPanelCphyMode>0</DsiPanelCphyMode>

		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>1</ColorAodDetectSupport>
		<!--horizontal/vertical line test-->
		<VtcLineSupport>1</VtcLineSupport>
		<VtcLineNoReset>1</VtcLineNoReset>
		<!--checksum support-->
		<PanelLogoChecksumSupport>0</PanelLogoChecksumSupport>
		<PanelChecksumSupport>1</PanelChecksumSupport>
		<ChecksumStressTestSupport>0</ChecksumStressTestSupport>
		<ChecksumStressTestVdd>1280000</ChecksumStressTestVdd>
		<ChecksumStressTestMipi>550</ChecksumStressTestMipi>
		<!--pcd errflag check support-->
		<GpioPcdErrflag>0</GpioPcdErrflag>
		<PanelPcdErrflagCheckSupport>0</PanelPcdErrflagCheckSupport>
		<PcdCmdsSupport>1</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		-->
		<PcdCmdsSupportCompareMode>1</PcdCmdsSupportCompareMode>
		<!--Check panel power status test support-->
		<PanelCheckRegOnSupport>0</PanelCheckRegOnSupport>
		<PanelCheckRegOnSupportDsmReport>0</PanelCheckRegOnSupportDsmReport>
		<PanelCheckAodRegOnSupport>0</PanelCheckAodRegOnSupport>
		<!--Lcdkit fps support -->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--Lcdkit default fps -->
		<PanelDefaultFps>60</PanelDefaultFps>
		<!--Lcdkit fps support list -->
		<PanelSupportFpsList>"60, 90"</PanelSupportFpsList>
		<!--Ap aod support-->
		<ApAodSupport>1</ApAodSupport>
		<DozeTimeDelay>1000</DozeTimeDelay>
		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>

		<!--bl min brightness setting-->
		<PanelBlmin>4</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--bl def brightness setting-->
		<PanelBldef>2047</PanelBldef>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<FactoryCheckRegSupport>0</FactoryCheckRegSupport>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--gpio offset number setting-->
		<PanelGpioOffset>308</PanelGpioOffset>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 1
				9, 1, 1
				3, 1, 1
				1, 1, 10
				5, 1, 10
				7, 1, 0
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>

		<!--Power off timming-->
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0
				5, 0, 1
				1, 0, 1
				9, 0, 1
				3, 0, 1
				2, 0, 1"</PowerOffStage>
		<OnlyPowerOffStage>"0, 0, 0"</OnlyPowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		when mode is regulate to set min voltage, max voltage, enable load, disable load
		mode, number, amplitude, min voltage, max voltage, enable load, disable load
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		min voltage:1850000-1.85v
		max voltage:5500000-5.5v
		enable load:62000
		disable load:80
		-->
		<LcdReset>"2, 24, 0"</LcdReset>
		<LcdIovcc>"1, 12, 1800000, 1800000, 1800000, 60700, 80"</LcdIovcc>
		<LcdVci>"1, 13, 3000000, 3000000, 3000000, 10000, 80"</LcdVci>
		<LcdVsp>"1, 1, 1200000, 1200000, 1200000, 60700, 80"</LcdVsp>
		<LcdVdd>"2, 49, 1200000"</LcdVdd>
		<IovccName>"vddio"</IovccName>
		<VciName>"vdd"</VciName>
		<VspName>"ddic"</VspName>
		<!--event send-->
		<!--Fp Delay Threshold-->
		<LeftTimeThUs>27667</LeftTimeThUs>
		<RightTimeThUs>29667</RightTimeThUs>
		<TeInervalTimeUs>16667</TeInervalTimeUs>
	</PanelEntry>
</hwlcd>
