<hwlcd>
	<Version>1.1</Version>

	<!-- any supported lcd Panel, add it here! very important -->
	<lcdlist>
		<!-- format: -->
		<!-- panel-name, gpio, brdid(product_id), product_name, product_floder, version_floder -->
		<!-- panel-name : must same with panel xml file name -->
		<!-- product & version folder: must same with the folder name or 'def' -->
		<lcd>320-a03-6p6-qcom,                          0x01,   1000,    pku,    pku,  def     </lcd>
		<lcd>330-a03-6p6-qcom,                          0x02,   1000,    pku,    pku,  def     </lcd>
		<lcd>250-a03-6p6-qcom,                          0x04,   1000,    pku,    pku,  def     </lcd>
		<lcd>320-a03-6p6-b6-qcom,                       0x06,   1000,    pku,    pku,  def     </lcd>
		<lcd>250-d00-6p6-qcom,                          0x08,   1000,    pku,    pku,  def     </lcd>
		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   1000,    pku,    pku,  def     </lcd>
		<lcd>320-a03-6p6-qcom,                          0x01,   1100,    kon,    kon,  def     </lcd>
		<lcd>330-a03-6p6-qcom,                          0x02,   1100,    kon,    kon,  def     </lcd>
		<lcd>250-a03-6p6-qcom,                          0x04,   1100,    kon,    kon,  def     </lcd>
		<lcd>320-a03-6p6-b6-qcom,                       0x06,   1100,    kon,    kon,  def     </lcd>
		<lcd>250-d00-6p6-qcom,                          0x08,   1100,    kon,    kon,  def     </lcd>
		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   1100,    kon,    kon,  def     </lcd>
		<lcd>290-20c-6p6-qcom,                          0x00,   3000,    nen,    nen,  def     </lcd>
		<lcd>110-20c-6p6-qcom,                          0x01,   3000,    nen,    nen,  def     </lcd>
		<lcd>190-20c-6p6-qcom,                          0x04,   3000,    nen,    nen,  def     </lcd>
		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   3000,    nen,    nen,  def     </lcd>
		<lcd>110-208-6p7-qcom,                          0x05,   3001,    ntn,    ntn,  def     </lcd>
