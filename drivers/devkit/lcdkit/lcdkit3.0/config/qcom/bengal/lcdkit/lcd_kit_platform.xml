		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   3001,    ntn,    ntn,  def     </lcd>
		<lcd>190-503-10p1-qcom,                         0x01,   4000,    grf3,   grf3, def     </lcd>
		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   4000,    grf3,   grf3, def     </lcd>
		<lcd>190-601-9p7-qcom,                          0x01,   4001,    grf,    grf,  def     </lcd>
		<lcd>190-604-9p7-qcom,                          0x04,   4001,    grf,    grf,  def     </lcd>
		<lcd>auo-otm1901a-5p2-1080p-video-default,      0x0A,   4001,    grf,    grf,  def     </lcd>
		<lcd>190-601-8p1-qcom,                          0x04,   5000,    kob2q,  kob2q,def     </lcd>
