		<PanelVersionSupport>1</PanelVersionSupport>
		<!--Hbm support-->
		<PanelHbmCtrlSupport>1</PanelHbmCtrlSupport>
		<PanelHbmFpCaptureSupport>0</PanelHbmFpCaptureSupport>
		<PanelHbmFpPrepareSupport>1</PanelHbmFpPrepareSupport>
