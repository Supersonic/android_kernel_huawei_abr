		<MipiClkPostAdjust>210</MipiClkPostAdjust>
		<MipiDataTHsPrepareAdjust>15</MipiDataTHsPrepareAdjust>
		<MipiDataTHsTrailAdjust>4294967274</MipiDataTHsTrailAdjust>
		<MipiDataLaneLp2HsTimeAdjust>4294967248</MipiDataLaneLp2HsTimeAdjust>
		<MipiClkTHsTrailAdjust>4294967284</MipiClkTHsTrailAdjust>
