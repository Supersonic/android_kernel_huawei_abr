		<MipiVBackPorch>58</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>45</MipiVFrontPorch>
		<!--Kirin 990 Mipi porch config end-->
		<!--Panel information config start-->
