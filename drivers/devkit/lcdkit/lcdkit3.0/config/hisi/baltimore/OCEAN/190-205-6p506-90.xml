		<PanelLogoChecksumSupport>0</PanelLogoChecksumSupport>
		<PanelChecksumSupport>1</PanelChecksumSupport>
		<ChecksumStressTestSupport>0</ChecksumStressTestSupport>
		<ChecksumStressTestVdd>1280000</ChecksumStressTestVdd>
		<ChecksumStressTestMipi>550</ChecksumStressTestMipi>
		<!--pcd errflag check support-->
		<GpioPcdErrflag>0</GpioPcdErrflag>
		<PanelPcdErrflagCheckSupport>0</PanelPcdErrflagCheckSupport>
		<!--pcd cmd check support-->
		<PcdCmdsSupport>1</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		2: mask 7th bit  == ok , != NG-->
		<PcdCmdsSupportCompareMode>2</PcdCmdsSupportCompareMode>
		<PcdValueMask>0x80</PcdValueMask>
		<PcdDetNum>30</PcdDetNum>
		<!--horizontal/vertical line test-->
		<VtcLineSupport>1</VtcLineSupport>
		<HorLineSupport>1</HorLineSupport>
		<HorLineDuration>600</HorLineDuration>
		<!--backlight delay adjust support-->
		<BacklightDelayAdjustSupport>1</BacklightDelayAdjustSupport>
		<BacklightDelayAdjust>0</BacklightDelayAdjust>
		<!--This configuration is enabled.
		You must first confirm the address of the platform of the hard reset time.
		The default configuration query address of 990 is 0x2e9, which is configured in PwrKeyLPressConfAddr.
		-->
		<PwrKeyLPressPwrOffSupport>1</PwrKeyLPressPwrOffSupport>
		<!--ddic low voltage detect test-->
		<DdicLVDetectSupport>1</DdicLVDetectSupport>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early-event, 8-TS later-event, 9-vdd
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 1
				9, 1, 1
				1, 1, 10"</PowerOnStage>
		<LPOnStage>"5, 1, 10
				7, 1, 0
				6, 1, 0
				8, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"5, 0, 0
				1, 0, 1
				9, 0, 1
				2, 0, 1"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdVci>"1, 12, 3100000"</LcdVci>
		<LcdIovcc>"1, 4, 1850000"</LcdIovcc>
		<LcdVdd>"1, 29, 1200000"</LcdVdd>
		<LcdVsp>"0, 0, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>
		<LcdReset>"2, 60, 0"</LcdReset>
		<LcdTe0>"2, 3, 0"</LcdTe0>
		<LcdBacklight>"0, 0, 0"</LcdBacklight>
		<!--vdd regulator-->
		<VddRegulator>ldo29</VddRegulator>
		<!--Vci regulator-->
		<VciRegulator>ldo12</VciRegulator>
		<!--event send-->
		<!--Effect config start-->
		<!--Gamma calibration support-->
		<PanelDynamicGammaSupport>0</PanelDynamicGammaSupport>
		<!--Gamma support-->
		<GammaSupport>1</GammaSupport>
		<!--Gmp support-->
		<GmpSupport>1</GmpSupport>
		<!--for hiace parameter-->
		<HiaceSupport>0</HiaceSupport>
