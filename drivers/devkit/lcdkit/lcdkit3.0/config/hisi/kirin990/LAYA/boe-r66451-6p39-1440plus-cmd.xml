<hwlcd>
	<PanelEntry>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>12</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>12</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>12</HPulseWidth>
		<!--VBP setting-->
		<VBackPorch>60</VBackPorch>
		<!--VFP setting-->
		<VFrontPorch>16</VFrontPorch>
		<!--VS setting-->
		<VPulseWidth>28</VPulseWidth>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>16</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>320</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>3</PanelPxlClkDiv>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>9</PanelIfbcType>
		<!--Vesa parameters-->
		<VesaSliceWidth>719</VesaSliceWidth>
		<VesaSliceHeight>29</VesaSliceHeight>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<PanelVsynCtrType>0</PanelVsynCtrType>
		<!--Pt station test support-->
		<PanelPtTestSupport>0</PanelPtTestSupport>

		<!--set vss test support-->
		<PanelSetVssSupport>0</PanelSetVssSupport>
		<CheckThreadEnable>0</CheckThreadEnable>

		<!--Ldo check test-->
		<LdoCheckSupport>1</LdoCheckSupport>
		<LdoCheckName>"ldo4"</LdoCheckName>
		<LdoCheckChannel>2</LdoCheckChannel>
		<LdoCheckThreshold>250</LdoCheckThreshold>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--Esd enable-->
		<PanelEsdSupport>0</PanelEsdSupport>
		<!--Sensorhub aod support-->
		<SensorhubAodSupport>0</SensorhubAodSupport>
		<MaskDelayTimeBeforeFp>0</MaskDelayTimeBeforeFp>
		<MaskDelayTimeAfterFp>0</MaskDelayTimeAfterFp>
		<BlDelayFrame>0</BlDelayFrame>
		<!--Local region update support-->
		<PanelDirtyRegionSupport>0</PanelDirtyRegionSupport>
		<!--Left align-->
		<DirtyLeftAlign>0xffff</DirtyLeftAlign>
		<!--Right align-->
		<DirtyRightAlign>0xffff</DirtyRightAlign>
		<!--Top align-->
		<DirtyTopAlign>30</DirtyTopAlign>
		<!--Bottom align-->
		<DirtyBottAlign>0xffff</DirtyBottAlign>
		<!--Width align-->
		<DirtyWidthAlign>0xffff</DirtyWidthAlign>
		<!--Height align-->
		<DirtyHeightAlign>30</DirtyHeightAlign>
		<!--Width Region-->
		<DirtyWidthMin>1440</DirtyWidthMin>
		<!--Height Region-->
		<DirtyHeightMin>30</DirtyHeightMin>
		<!--Top location-->
		<DirtyTopStart>0xffff</DirtyTopStart>
		<!--Bottom location-->
		<DirtyBottStart>0xffff</DirtyBottStart>
		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>8</PanelBlType>
		<!--bl ic type:1-reg only, 2-pwm only, 3-muti-ramp, 4-ramp-muti, 6-i2c only, 7-blpwm-cabc-->
		<PanelBlIcCtrlType>0</PanelBlIcCtrlType>
		<!--bl min brightness setting-->
		<PanelBlmin>121</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>1638</PanelBlDef>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--mipi dsi support-->
		<MipiDsiUptSupport>1</MipiDsiUptSupport>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>500</MipiDsiBitClk>
		<!--Mipi dynamic clk_1-->
		<MipiDsiBitClkValA>480</MipiDsiBitClkValA>
		<!--Mipi dynamic clk_2-->
		<MipiDsiBitClkValB>500</MipiDsiBitClkValB>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>230</MipiClkPostAdjust>
		<MipiClkTLpxAdjust>4294967232</MipiClkTLpxAdjust>
		<MipiDataTLpxAdjust>4294967232</MipiDataTLpxAdjust>
		<!--Hbm support-->
		<PanelHbmCtrlSupport>1</PanelHbmCtrlSupport>
		<!--checksum support-->
		<PanelChecksumSupport>0</PanelChecksumSupport>
		<!--panel version support-->
		<PanelVersionSupport>0</PanelVersionSupport>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 5
				1, 1, 5"</PowerOnStage>
		<LPOnStage>"5, 1, 5
				9, 1, 5
				7, 1, 0
				6, 1, 0
				8, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"5, 0, 10
				9, 0, 5
				1, 0, 1
				2, 0, 1"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdVci>"2, 246, 0"</LcdVci>
		<LcdIovcc>"1, 4, 1850000"</LcdIovcc>
		<LcdVdd>"1, 29, 1250000"</LcdVdd>
		<LcdVsp>"2, 0, 5500000"</LcdVsp>
		<LcdVsn>"2, 0, 5500000"</LcdVsn>
		<LcdReset>"2, 279, 0"</LcdReset>
		<TpReset>"2, 0, 0"</TpReset>
		<LcdTe0>"2, 0, 0"</LcdTe0>
		<LcdBacklight>"2, 0, 0"</LcdBacklight>
		<VddRegulator>ldo29</VddRegulator>
		<!--event send-->
		<!--Effect config start-->
		<DoesLcdPowerOnTp>0</DoesLcdPowerOnTp>
		<!--Gamma support-->
		<GammaSupport>0</GammaSupport>
		<!--Otp Gamma support-->
		<OtpGammaSupport>0</OtpGammaSupport>
		<!--Gmp support-->
		<GmpSupport>0</GmpSupport>
		<!--for hiace parameter-->
		<HiaceSupport>0</HiaceSupport>
		<IGlobalHistBlackPos>16</IGlobalHistBlackPos>
		<IGlobalHistWhitePos>240</IGlobalHistWhitePos>
		<IGlobalHistBlackWeight>51</IGlobalHistBlackWeight>
		<IGlobalHistWhiteWeight>51</IGlobalHistWhiteWeight>
		<IGlobalHistZeroCutRatio>486</IGlobalHistZeroCutRatio>
		<IGlobalHistSlopeCutRatio>410</IGlobalHistSlopeCutRatio>
		<IMaxLcdLuminance>500</IMaxLcdLuminance>
		<IMinLcdLuminance>3</IMinLcdLuminance>
		<CfgName>"/product/etc/display/effect/algorithm/hdr_engine.xml"</CfgName>
		<!--Xcc support-->
		<XccSupport>0</XccSupport>
		<Arsr1pSharpnessSupport>0</Arsr1pSharpnessSupport>
		<!--prefix 2D sharpness support-->
		<PrefixSharpTwoDSupport>0</PrefixSharpTwoDSupport>
		<!--Effect config end-->
		<PanelHbmCommand>"0x39, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x03, 0x51, 0x07, 0xff"</PanelHbmCommand>
		<PanelHbmExitCommand>"0x39, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x03, 0x51, 0x01, 0x8D"</PanelHbmExitCommand>
	</PanelEntry>
</hwlcd>
