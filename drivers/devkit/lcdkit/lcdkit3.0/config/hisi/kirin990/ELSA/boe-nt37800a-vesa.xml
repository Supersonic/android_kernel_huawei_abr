		<DirtyLeftAlign>0xffff</DirtyLeftAlign>
		<!--Right align-->
		<DirtyRightAlign>0xffff</DirtyRightAlign>
		<!--Top align-->
		<DirtyTopAlign>44</DirtyTopAlign>
		<!--Bottom align-->
		<DirtyBottAlign>0xffff</DirtyBottAlign>
		<!--Width align-->
		<DirtyWidthAlign>0xffff</DirtyWidthAlign>
		<!--Height align-->
		<DirtyHeightAlign>44</DirtyHeightAlign>
		<!--Width Region-->
		<DirtyWidthMin>1200</DirtyWidthMin>
		<!--Height Region-->
		<DirtyHeightMin>44</DirtyHeightMin>
		<!--Top location-->
		<DirtyTopStart>0xffff</DirtyTopStart>
		<!--Bottom location-->
		<DirtyBottStart>0xffff</DirtyBottStart>
