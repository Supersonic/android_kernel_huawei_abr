<hwlcd>
	<PanelEntry>
		<MipiDsiTimingSupport>1</MipiDsiTimingSupport>
		<!--Mipi Horizontal sync area -->
		<MipiHSyncArea>11</MipiHSyncArea>
		<!--Mipi Horizontal back porch -->
		<MipiHBackPorch>37</MipiHBackPorch>
		<!--Mipi Horizontal line time -->
		<MipiHLineTime>936</MipiHLineTime>
		<!--Mipi Dpi horizontal size -->
		<MipiDpiHSize>812</MipiDpiHSize>
		<!--Mipi Vertical sync area -->
		<MipiVSyncArea>10</MipiVSyncArea>
		<!--Mipi Vertical back porch -->
		<MipiVBackPorch>54</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>36</MipiVFrontPorch>
		<!--Mipi porch config end-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>8</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>192</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>1</PanelPxlClkDiv>
		<PanelDSCEnable>0</PanelDSCEnable>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>0</PanelIfbcType>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<!--Vesa parameters-->
		<VesaSliceWidth>539</VesaSliceWidth>
		<VesaSliceHeight>19</VesaSliceHeight>
		<PanelVsynCtrType>0</PanelVsynCtrType>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<PanelOnAlwaysNeedReset>1</PanelOnAlwaysNeedReset>
		<!--Panel display effect support-->
		<PanelDisplayOnEffectSupport>0</PanelDisplayOnEffectSupport>
		<PanelGetBlMaxNitType>1</PanelGetBlMaxNitType>
		<PanelGetBlMaxNitOffset>420</PanelGetBlMaxNitOffset>
		<!--Ldo check test-->
		<LdoCheckSupport>0</LdoCheckSupport>
		<LdoCheckName>"ldo4"</LdoCheckName>
		<LdoCheckChannel>2</LdoCheckChannel>
		<LdoCheckThreshold>250</LdoCheckThreshold>
		<!--Panel quickly sleep out support-->
		<PanelQuicklySleepOutSupport>1</PanelQuicklySleepOutSupport>
		<PanelQuicklySleepOutInterval>70</PanelQuicklySleepOutInterval>
		<!--Project id -->
		<PanelProjectIdSupport>1</PanelProjectIdSupport>
		<PanelDefaultTpProjectId>"G1929M1200"</PanelDefaultTpProjectId>
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>1</PanelInversionSupport>
		<!--Forward/reverse scan test support-->
		<PanelScanSupport>0</PanelScanSupport>
		<!--THP Proximity support-->
		<PanelThpProximitySupport>1</PanelThpProximitySupport>
		<!-- mipi check -->
		<PanelMipiCheckSupport>0</PanelMipiCheckSupport>
		<!--Oem info-->
		<OemInfoSupport>1</OemInfoSupport>
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<OemBarcode2DBlockNum>4</OemBarcode2DBlockNum>
		<PanelBrightnessColorUniformSupport>0</PanelBrightnessColorUniformSupport>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--sn code check-->
		<PanelSnCodeCheck>1</PanelSnCodeCheck>
		<!--sn code info define
		mode info, glass info, cg info, bl info
		-->
		<LcdInfo>"9, 20, 34, 38"</LcdInfo>
		<!--rgbw support-->
		<PanelRgbwSupport>0</PanelRgbwSupport>
		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
		<!--Esd timing ctrl: 1: vactive start to send esd cmd-->
		<PanelEsdTimingCtrl>1</PanelEsdTimingCtrl>
		<!--Local region update support-->
		<PanelDirtyRegionSupport>0</PanelDirtyRegionSupport>
		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>4</PanelBlType>
		<!--bl ic type:1-reg only, 2-pwm only, 3-muti-ramp, 4-ramp-muti, 6-i2c only, 7-blpwm-cabc-->
		<PanelBlIcCtrlType>6</PanelBlIcCtrlType>
		<!--bl max nit-->
		<PanelBlMaxnit>480</PanelBlMaxnit>
		<!-- 0:TP flash  1:DDIC -->
		<PanelGetBlMaxNitType>1</PanelGetBlMaxNitType>
		<!--bl min brightness setting-->
		<PanelBlmin>25</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>6600</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>4000</PanelBlDef>
		<!--blpwm PreciType setting-->
		<PanelBlPwmPreciType>1</PanelBlPwmPreciType>
		<!--blpwm intput disable, used for AP-cabc-->
		<BlPwmInputDisable>1</BlPwmInputDisable>
		<!--Panel Check Reg Support-->
		<PanelCheckRegSupport>1</PanelCheckRegSupport>
		<PanelCheckRegOnSupport>1</PanelCheckRegOnSupport>
		<PanelCheckRegOffSupport>0</PanelCheckRegOffSupport>
		<!--Bl Ic Ctrl Type: 1-REG_ONLY_MODE, 2-PWM_ONLY_MODE, 3-MUTI_THEN_RAMP_MODE, 4-RAMP_THEN_MUTI_MODE, 6-I2C_ONLY_MODE-->
		<!--Bl Ic Ctrl Type: 7-BLPWM_AND_CABC_MODE, 8-COMMON_IC_MODE, 9-AMOLED_NO_BL_IC_MODE, 10-BLPWM_MODE-->
		<PanelBlIcCtrlType>6</PanelBlIcCtrlType>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--Mipi clk dynamic update support-->
		<MipiDsiUptSupport>1</MipiDsiUptSupport>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>562</MipiDsiBitClk>
		<!--Mipi burst mode:0-non burst sync pulse, 1-non burst sync event, 2-burst sync pulse1, 3-burst sync pulse2-->
		<MipiBurstMode>2</MipiBurstMode>
		<!--Mipi non continue enable:0-continue mode, 1-non continue mode-->
		<MipiNonContinueEnable>0</MipiNonContinueEnable>
		<!--Mipi lp11:0-None, 1-little lp11, 2-no lp11-->
		<MipiLp11Flag>1</MipiLp11Flag>
		<!--Mipi dynamic clk_1-->
		<MipiDsiBitClkValA>567</MipiDsiBitClkValA>
		<!--Mipi dynamic clk_2-->
		<MipiDsiBitClkValB>562</MipiDsiBitClkValB>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>0</MipiClkPostAdjust>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early-event, 8-TS later-event, 9-VDD, 10-AOD, 11-BIAS
		data:0-power off, 1-power on, 2-power on and reset low
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 0
				11, 1, 5
				3, 1, 1
				4, 1, 10"</PowerOnStage>
		<LPOnStage>"5, 1, 5
				5, 2, 1
				5, 1, 5
				5, 2, 1
				5, 1, 10
				7, 1, 2
				6, 1, 0"</LPOnStage>
		<HSOnStage>"8, 1, 0"</HSOnStage>
		<ThpProximityResetDelayMin>65</ThpProximityResetDelayMin>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"0, 0, 5
				5, 0, 60
				11, 0, 5
				4, 0, 1
				3, 0, 1
				2, 0, 100"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdIovcc>"1, 4, 1800000"</LcdIovcc>
		<LcdVsp>"2, 36, 5500000"</LcdVsp>
		<LcdVsn>"2, 37, 5500000"</LcdVsn>
		<PowerDownVsp>"2, 36, 4700000"</PowerDownVsp>
		<PowerDownVsn>"2, 37, 4700000"</PowerDownVsn>
		<LcdReset>"2, 42, 0"</LcdReset>
		<TpReset>"2, 187, 0"</TpReset>
		<LcdTe0>"2, 2, 0"</LcdTe0>
		<!--event send-->
		<!--Effect config start-->
		<!--Cabc support-->
		<PanelCabcSupport>0</PanelCabcSupport>
		<!--Gamma support-->
		<GammaSupport>1</GammaSupport>
		<!--Gmp support-->
		<GmpSupport>1</GmpSupport>
		<!--Gamma calibration support-->
		<PanelDynamicGammaSupport>0</PanelDynamicGammaSupport>
		<!--ComformMode support-->
		<ComformModeSupport>1</ComformModeSupport>
		<!--Color temperature support-->
		<ColorTempSupport>1</ColorTempSupport>
		<!--for hiace parameter-->
		<HiaceSupport>1</HiaceSupport>
		<IGlobalHistBlackPos>16</IGlobalHistBlackPos>
		<IGlobalHistWhitePos>240</IGlobalHistWhitePos>
		<IGlobalHistBlackWeight>51</IGlobalHistBlackWeight>
		<IGlobalHistWhiteWeight>51</IGlobalHistWhiteWeight>
		<IGlobalHistZeroCutRatio>486</IGlobalHistZeroCutRatio>
		<IGlobalHistSlopeCutRatio>410</IGlobalHistSlopeCutRatio>
		<IMaxLcdLuminance>500</IMaxLcdLuminance>
		<IMinLcdLuminance>3</IMinLcdLuminance>
		<CfgName>"/product/etc/display/effect/algorithm/hdr_engine.xml"</CfgName>
		<!--Xcc support-->
		<XccSupport>1</XccSupport>
		<!--Post Xcc support-->
		<PostXccSupport>1</PostXccSupport>
		<Arsr1pSharpnessSupport>1</Arsr1pSharpnessSupport>
		<!--prefix 2D sharpness support-->
		<PrefixSharpTwoDSupport>1</PrefixSharpTwoDSupport>
		<!--Gmp lut -->
		<!--Effect config end-->
		<!--vertical line test-->
		<VtcLineSupport>1</VtcLineSupport>
		<VtcLineVsp>6000000</VtcLineVsp>
		<VtcLineVsn>6000000</VtcLineVsn>
		<VtcLineNoReset>1</VtcLineNoReset>

		<!--Fps fuction-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--MMI test fps func switch-->
		<FpsSwitchTestSupport>1</FpsSwitchTestSupport>
		<FpsEffectDelayFrameCnt>0</FpsEffectDelayFrameCnt>
		<FpsNeedHigh60Mode>1</FpsNeedHigh60Mode>
		<FpsDisableDsiClkUpt>1</FpsDisableDsiClkUpt>
		<!--bit0:hporch bit1:vporch bit2:clk-->
		<FpsSwitchMode>7</FpsSwitchMode>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
			4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
			8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<FpsDynamicIfbcType>9</FpsDynamicIfbcType>
		<!--panel switch to normal 60hz code-->
		<PanelFpsNormal60Command>"0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x25
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x18, 0x21
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x10"</PanelFpsNormal60Command>
		<FpsToNormal60CmdState>1</FpsToNormal60CmdState>
		<!--panel switch to high 60hz code-->
		<PanelFpsToSixtyCommand>"0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x25
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x18, 0x20
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x10"</PanelFpsToSixtyCommand>
		<FpsTo60CmdState>1</FpsTo60CmdState>
		<!--panel switch to 90hz code-->
		<FpsTo90Command>"0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x25
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0x18, 0x20
			0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xFF, 0x10"</FpsTo90Command>
		<FpsTo90CmdState>1</FpsTo90CmdState>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsToNormal60DsiTimming>"11, 37, 812, 936, 10, 54, 36, 562"</FpsToNormal60DsiTimming>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo60DsiTimming>"12, 117, 272, 528, 10, 54, 1286, 476"</FpsTo60DsiTimming>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo90DsiTimming>"12, 117, 272, 528, 10, 54, 36, 476"</FpsTo90DsiTimming>
		<!--frame rate 1->delay->...->frame rate n->delay-->
		<FpsSwitchOrder>"90, 200, 60, 200"</FpsSwitchOrder>
		<!--frame rate 1->...->frame rate n-->
		<PanelSupportFpsList>"60, 90"</PanelSupportFpsList>
	</PanelEntry>
</hwlcd>
