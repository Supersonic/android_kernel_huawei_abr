<hwlcd>
	<PanelEntry>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>38</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>81</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>6</HPulseWidth>
		<!--VBP setting-->
		<VBackPorch>34</VBackPorch>
		<!--VFP setting-->
		<VFrontPorch>2444</VFrontPorch>
		<!--VS setting-->
		<VPulseWidth>2</VPulseWidth>
		<!--LP VFP setting-->
		<VFrontPorchForLP>2444</VFrontPorchForLP>
		<!--VReFresh setting-->
		<VReFresh>60</VReFresh>
		<!--Fps fuction-->
		<PanelFpsUpdtSupport>0</PanelFpsUpdtSupport>
		<!-- 0-60hz, 1-H60hz, 2-90hz, 3-120hz, first is default -->
		<PanelSupportFpsList>"1, 3"</PanelSupportFpsList>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE-RATE-LOWER -->
		<FpsToNormal60DsiTimming>"81, 38, 6, 2444, 34, 2, 60, 952, 0"</FpsToNormal60DsiTimming>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE-RATE-LOWER -->
		<FpsToHigh60DsiTimming>"81, 38, 6, 2444, 34, 2, 60, 952, 0"</FpsToHigh60DsiTimming>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE-RATE-LOWER -->
		<FpsTo90DsiTimming>"81, 38, 6, 2444, 34, 2, 90, 952, 0"</FpsTo90DsiTimming>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE-RATE-LOWER -->
		<FpsTo120DsiTimming>"81, 38, 6, 64, 34, 2, 120, 952, 0"</FpsTo120DsiTimming>
		<!--Mipi hopping support-->
		<MipiHoppingSupport>0</MipiHoppingSupport>
		<!-- HFP-HBP-HS-VFP-VBP-VS-RATE-LOWER -->
		<FpsToHigh60MipiHopping>"123, 76, 6, 1226, 62, 2, 812, 0"</FpsToHigh60MipiHopping>
		<!-- HFP-HBP-HS-VFP-VBP-VS-RATE-LOWER -->
		<FpsTo90MipiHopping>"123, 76, 6, 36, 62, 2, 812, 0"</FpsTo90MipiHopping>
		<!-- HFP-HBP-HS-VFP-VBP-VS-RATE-LOWER -->
		<FpsTo120MipiHopping>"123, 76, 6, 36, 62, 2, 812, 0"</FpsTo120MipiHopping>

		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"P1749Y130"</PanelDefaultTpProjectId>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:1-1lane, 2-2lane, 3-3lane, 4-4lane-->
		<MipiLaneNums>4</MipiLaneNums>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>8</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>0</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>0</PanelPxlClkDiv>
		<!--mipi data rate pixel clock * 2-->
		<PanelDataRate>952</PanelDataRate>
		<!--Mipi non continue enable:0-continue mode, 1-non continue mode-->
		<MipiNonContinueEnable>1</MipiNonContinueEnable>
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>0</MipiClkPostAdjust>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>1</PanelGetBlMaxNitType>
		<PanelGetBlMaxNitOffset>640</PanelGetBlMaxNitOffset>
		<PanelThpProximitySupport>0</PanelThpProximitySupport>
		<!--bl max nit-->
		<PanelBlMaxnit>820</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>18</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--bl ctrl type setting-->
		<PanelBlIcCtrlType>8</PanelBlIcCtrlType>
		<!--bias ctrl type setting-->
		<PanelBiasIcCtrlType>0</PanelBiasIcCtrlType>
		<!--gpio offset number setting-->
		<PanelGpioOffset>284</PanelGpioOffset>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>1</PanelInversionSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegSupport>1</PanelCheckRegSupport>
		<!--Project id support-->
		<PanelProjectIdSupport>1</PanelProjectIdSupport>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>1</PanelOnAlwaysNeedReset>
		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--Panel quickly sleep out support-->
		<PanelQuicklySleepOutSupport>1</PanelQuicklySleepOutSupport>
		<PanelQuicklySleepOutInterval>102</PanelQuicklySleepOutInterval>
		<!--rgbw support-->
		<PanelRgbwSupport>0</PanelRgbwSupport>
		<IMaxLcdLuminance>500</IMaxLcdLuminance>
		<IMinLcdLuminance>3</IMinLcdLuminance>
		<DoesLcdPowerOnTp>1</DoesLcdPowerOnTp>
		<!-- DSC params -->
		<DSCEnable>1</DSCEnable>
		<DSCVer>17</DSCVer>
		<DSCSliceMode>1</DSCSliceMode>
		<DSCRgbSwap>0</DSCRgbSwap>
		<DSCCfg>34</DSCCfg>
		<DSCRctOn>1</DSCRctOn>
		<DSCBitPerChannel>8</DSCBitPerChannel>
		<DSCLineBufDepth>9</DSCLineBufDepth>
		<DSCBpEnable>1</DSCBpEnable>
		<DSCBitPerPixel>128</DSCBitPerPixel>
		<DSCPicHeight>2280</DSCPicHeight>
		<DSCPicWidth>1080</DSCPicWidth>
		<DSCSliceHeight>20</DSCSliceHeight>
		<DSCSliceWidth>540</DSCSliceWidth>
		<DSCChunkSize>540</DSCChunkSize>
		<DSCXmitDelay>512</DSCXmitDelay>
		<DSCDecDelay>526</DSCDecDelay>
		<DSCScaleValue>32</DSCScaleValue>
		<DSCIncrementInterval>488</DSCIncrementInterval>
		<DSCDecrementInterval>7</DSCDecrementInterval>
		<DSCLineBpgOffset>12</DSCLineBpgOffset>
		<DSCNflBpgOffset>1294</DSCNflBpgOffset>
		<DSCSliceBpgOffset>1302</DSCSliceBpgOffset>
		<DSCInitialOffset>6144</DSCInitialOffset>
		<DSCFinalOffset>4336</DSCFinalOffset>
		<DSCFlatnessMinqp>3</DSCFlatnessMinqp>
		<DSCFlatnessMaxqp>12</DSCFlatnessMaxqp>
		<DSCRcModelSize>8192</DSCRcModelSize>
		<DSCRcEdgeFactor>6</DSCRcEdgeFactor>
		<DSCRcQuantIncrLimit0>11</DSCRcQuantIncrLimit0>
		<DSCRcQuantIncrLimit1>11</DSCRcQuantIncrLimit1>
		<DSCRcTgtOffsetHi>3</DSCRcTgtOffsetHi>
		<DSCRcTgtOffsetLo>3</DSCRcTgtOffsetLo>

		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"11, 1, 0
				3, 1, 2
				4, 1, 10
				5, 2, 1
				5, 1, 5
				5, 2, 1
				5, 1, 10
				7, 1, 2
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"0, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				8, 0, 30
				6, 0, 0
				4, 0, 1
				3, 0, 2"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdVci>"0, 0, 0"</LcdVci>
		<LcdIovcc>"0, 0, 1800000"</LcdIovcc>
		<LcdVsp>"2, 28, 5500000"</LcdVsp>
		<LcdVsn>"2, 29, 5500000"</LcdVsn>
		<LcdReset>"2, 42, 0"</LcdReset>
		<!--bl max nit-->
		<PanelBlMaxNitCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0xdb"</PanelBlMaxNitCommand>
		<PanelBlMaxNitCommandState>0</PanelBlMaxNitCommandState>
	</PanelEntry>
</hwlcd>
