<hwlcd>
	<PanelEntry>
		<!-- Panel Infomation -->
		<PanelCompatible>"090_703_6p53"</PanelCompatible>
		<PanelName>"090_703 6.53' 1080 x 2400"</PanelName>
		<PanelType>0x02</PanelType>
		<PanelXres>1080</PanelXres>
		<PanelYres>2400</PanelYres>
		<PanelWidth>70</PanelWidth>
		<PanelHeight>155</PanelHeight>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>22</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>80</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>16</HPulseWidth>
		<!--VBP setting-->
		<VBackPorch>7</VBackPorch>
		<!--VFP setting-->
		<VFrontPorch>22</VFrontPorch>
		<!--VS setting-->
		<VPulseWidth>3</VPulseWidth>

		<!--VReFresh setting-->
		<VReFresh>60</VReFresh>
		<!--Fps fuction-->
		<PanelFpsUpdtSupport>0</PanelFpsUpdtSupport>
		<!--DSC Params-->
		<DSCEnable>0</DSCEnable>

		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"B1859U0900"</PanelDefaultTpProjectId>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:1-1lane, 2-2lane, 3-3lane, 4-4lane-->
		<MipiLaneNums>4</MipiLaneNums>
		<!--Panel information config start-->
		<!--Pixel clock-->
		<PanelPxlClk>562</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>0</PanelPxlClkDiv>
		<!--mipi data rate pixel clock * 2-->
		<PanelDataRate>1124</PanelDataRate>
		<!--Mipi non continue enable:0-continue mode, 1-non continue mode-->
		<MipiNonContinueEnable>1</MipiNonContinueEnable>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
		<PanelSkipPowerOnEsdCheck>1</PanelSkipPowerOnEsdCheck>
		<PanelAgainEsdCheckDelay>140</PanelAgainEsdCheckDelay>
                <PanelEsdCheckDelay>100</PanelEsdCheckDelay>
		<!-- Esd recovery backlight -->
		<PanelEsdRecoveryBlSupport>1</PanelEsdRecoveryBlSupport>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>1</PanelGetBlMaxNitType>
		<!--bl max nit-->
		<PanelBlMaxnit>450</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>24</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>2047</PanelBlmax>
		<!--bl ctrl type setting-->
		<PanelBlIcCtrlType>8</PanelBlIcCtrlType>
		<!--bias ctrl type setting-->
		<PanelBiasIcCtrlType>0</PanelBiasIcCtrlType>
		<!--gpio offset number setting-->
		<PanelGpioOffset>325</PanelGpioOffset>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>1</PanelOnAlwaysNeedReset>
		<PanelPrePowerOff>1</PanelPrePowerOff>
		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<Barcode2DCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x04, 0xA1, 0x00, 0x00, 0x00
				0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xB0, 0x04
				0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x04, 0xA1, 0x00, 0x00, 0x00
				0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xB0, 0x08
				0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x04, 0xA1, 0x00, 0x00, 0x00
				0x15, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x02, 0xB0, 0x0C
				0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x04, 0xA1, 0x00, 0x00, 0x00"</Barcode2DCommand>

		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 2
				1, 1, 12
				5, 2, 1
				5, 1, 7
				7, 1, 0
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"0, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0
				0, 0, 0
				5, 0, 10
				1, 0, 1
				2, 0, 1"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<!--IOVccRegulator>ldo4</IOVccRegulator-->
		<LcdIovcc>"2, 166, 1800000"</LcdIovcc>
		<LcdVci>"1, 28, 3000000"</LcdVci>
		<VciRegulatorName>"vldo28"</VciRegulatorName>
		<LcdReset>"2, 45, 0"</LcdReset>
		<LcdVsp>"0, 0, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>

		<!--event send-->
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>0</MipiClkPostAdjust>
		<!--bl max nit-->
		<PanelBlMaxNitCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0xdb"</PanelBlMaxNitCommand>
		<PanelBlMaxNitCommandState>0</PanelBlMaxNitCommandState>
		<!--below added by xujibin-->
		<!--Sensorhub aod support-->
		<ApAodSupport>1</ApAodSupport>
                <ApAodDozeDelay>3</ApAodDozeDelay>
		<PanelAodNeedReset>1</PanelAodNeedReset>
		<PanelAodNoNeedInit>1</PanelAodNoNeedInit>
		<TeInervalTimeUs>16667</TeInervalTimeUs>
		<MaskDelayTimeBeforeFp>16000</MaskDelayTimeBeforeFp>
		<MaskDelayTimeAfterFp>16667</MaskDelayTimeAfterFp>
		<FPHBMEntryDelayAfterBL>90000</FPHBMEntryDelayAfterBL>
		<!--Hbm support-->
		<PanelHbmCtrlSupport>1</PanelHbmCtrlSupport>
		<!--Hbm 12 bit ctrl support-->
		<PanelHbmSpecialBitSupport>1</PanelHbmSpecialBitSupport>
		<!--Hbm FingerPrint capture support-->
		<PanelHbmFpCaptureSupport>1</PanelHbmFpCaptureSupport>
		<!--Hbm max level-->
		<PanelHbmLevelMax>16</PanelHbmLevelMax>
		<PanelHbmSetElvssDimLp>1</PanelHbmSetElvssDimLp>
		<PanelHbmElvssDimSupport>1</PanelHbmElvssDimSupport>
		<PanelHbmElvssDimCmdDelay>0</PanelHbmElvssDimCmdDelay>
	</PanelEntry>
</hwlcd>
