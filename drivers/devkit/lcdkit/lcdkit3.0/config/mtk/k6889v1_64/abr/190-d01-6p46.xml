<hwlcd>
	<PanelEntry>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>20</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>60</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>20</HPulseWidth>
		<!--VBP setting-->
		<VBackPorch>60</VBackPorch>
		<!--VFP setting-->
		<VFrontPorch>80</VFrontPorch>
		<!--VS setting-->
		<VPulseWidth>40</VPulseWidth>

		<!--VReFresh setting-->
		<VReFresh>60</VReFresh>
		<!-- 0:cmd,1:plus-vdo,2:event-mode,3:burst-vdo -->
		<LdiDsiMode>0</LdiDsiMode>
		<!--Fps fuction-->
		<PanelFpsUpdtSupport>0</PanelFpsUpdtSupport>
		<!-- 0-60hz, 1-H60hz, 2-90hz, 3-120hz, first is default -->
		<PanelSupportFpsList>"1, 2"</PanelSupportFpsList>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE-RATE-LOWER -->
		<FpsToHigh60DsiTimming>"60, 20, 20, 80, 60, 40, 60, 500, 0"</FpsToHigh60DsiTimming>
		<!-- HFP-HBP-HS-VFP-VBP-VS-VRE -->
		<FpsTo90DsiTimming>"60, 20, 20, 80, 60, 40, 90, 800, 0"</FpsTo90DsiTimming>
		<!--Mipi hopping support-->
		<MipiHoppingSupport>0</MipiHoppingSupport>

		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:1-1lane, 2-2lane, 3-3lane, 4-4lane-->
		<MipiLaneNums>4</MipiLaneNums>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>16</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>0</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>0</PanelPxlClkDiv>
		<!--mipi data rate pixel clock * 2-->
		<PanelDataRate>500</PanelDataRate>
		<!--Mipi non continue enable:0-continue mode, 1-non continue mode-->
		<MipiNonContinueEnable>1</MipiNonContinueEnable>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--Esd enable-->
		<PanelEsdSupport>0</PanelEsdSupport>
		<!--Skip power on ESD check-->
		<PanelSkipPowerOnEsdCheck>0</PanelSkipPowerOnEsdCheck>
		<!-- Esd recovery backlight -->
		<PanelEsdRecoveryBlSupport>0</PanelEsdRecoveryBlSupport>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>0</PanelGetBlMaxNitType>
		<!--bl max nit-->
		<PanelBlMaxnit>1000</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>3</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>8191</PanelBlmax>
		<!--bl ctrl type setting-->
		<PanelBlIcCtrlType>7</PanelBlIcCtrlType>
		<!--bias ctrl type setting-->
		<PanelBiasIcCtrlType>0</PanelBiasIcCtrlType>
		<!--gpio offset number setting-->
		<PanelGpioOffset>285</PanelGpioOffset>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>0</PanelOnAlwaysNeedReset>
		<!--OemInfo support-->
		<OemInfoSupport>0</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>0</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>0</PanelSnCodeSupport>

		<!-- DSC params -->
		<DSCEnable>1</DSCEnable>
		<DSCVer>17</DSCVer>
		<DSCSliceMode>0</DSCSliceMode>
		<DSCRgbSwap>0</DSCRgbSwap>
		<DSCCfg>34</DSCCfg>
		<DSCRctOn>1</DSCRctOn>
		<DSCBitPerChannel>8</DSCBitPerChannel>
		<DSCLineBufDepth>9</DSCLineBufDepth>
		<DSCBpEnable>1</DSCBpEnable>
		<DSCBitPerPixel>128</DSCBitPerPixel>
		<DSCPicHeight>2700</DSCPicHeight>
		<DSCPicWidth>1224</DSCPicWidth>
		<DSCSliceHeight>60</DSCSliceHeight>
		<DSCSliceWidth>1224</DSCSliceWidth>
		<DSCChunkSize>1224</DSCChunkSize>
		<DSCXmitDelay>512</DSCXmitDelay>
		<DSCDecDelay>970</DSCDecDelay>
		<DSCScaleValue>32</DSCScaleValue>
		<DSCIncrementInterval>1871</DSCIncrementInterval>
		<DSCDecrementInterval>17</DSCDecrementInterval>
		<DSCLineBpgOffset>14</DSCLineBpgOffset>
		<DSCNflBpgOffset>486</DSCNflBpgOffset>
		<DSCSliceBpgOffset>192</DSCSliceBpgOffset>
		<DSCInitialOffset>6144</DSCInitialOffset>
		<DSCFinalOffset>4336</DSCFinalOffset>
		<DSCFlatnessMinqp>3</DSCFlatnessMinqp>
		<DSCFlatnessMaxqp>12</DSCFlatnessMaxqp>
		<DSCRcModelSize>8192</DSCRcModelSize>
		<DSCRcEdgeFactor>6</DSCRcEdgeFactor>
		<DSCRcQuantIncrLimit0>11</DSCRcQuantIncrLimit0>
		<DSCRcQuantIncrLimit1>11</DSCRcQuantIncrLimit1>
		<DSCRcTgtOffsetHi>3</DSCRcTgtOffsetHi>
		<DSCRcTgtOffsetLo>3</DSCRcTgtOffsetLo>

		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"3, 1, 10
				2, 1, 1
				9, 1, 1
				1, 1, 10
				5, 1, 10
				7, 1, 0
				6, 1, 0
				8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"0, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 0
				8, 0, 0
				5, 0, 1
				1, 0, 1
				9, 0, 1
				2, 0, 1"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--backlight order-->
		<BacklightOrder>1</BacklightOrder>
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<!--IOVccRegulator>ldo4</IOVccRegulator-->
		<LcdIovcc>"2, 184, 1800000"</LcdIovcc>
		<LcdVci>"1, 1, 3100000"</LcdVci>
		<VciRegulatorName>"VFP"</VciRegulatorName>
		<LcdVdd>"2, 192, 1220000"</LcdVdd>
		<LcdReset>"2, 42, 0"</LcdReset>
		<LcdVsp>"2, 27, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>

		<!--event send-->
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>0</MipiClkPostAdjust>
		<!--bl max nit-->
		<PanelBlMaxNitCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0xdb"</PanelBlMaxNitCommand>
		<PanelBlMaxNitCommandState>0</PanelBlMaxNitCommandState>
		<!--below added by xujibin-->
		<!--Sensorhub aod support-->
		<ApAodSupport>0</ApAodSupport>
		<MaskDelayTimeBeforeFp>16000</MaskDelayTimeBeforeFp>
		<MaskDelayTimeAfterFp>16667</MaskDelayTimeAfterFp>
		<FPHBMEntryDelayAfterBL>90000</FPHBMEntryDelayAfterBL>
		<!--Hbm support-->
		<PanelHbmCtrlSupport>0</PanelHbmCtrlSupport>
		<!--Hbm 12 bit ctrl support-->
		<PanelHbmSpecialBitSupport>0</PanelHbmSpecialBitSupport>
		<!--Hbm FingerPrint capture support-->
		<PanelHbmFpCaptureSupport>0</PanelHbmFpCaptureSupport>
		<!--Hbm max level-->
		<PanelHbmLevelMax>16</PanelHbmLevelMax>
		<PanelHbmSetElvssDimLp>1</PanelHbmSetElvssDimLp>
		<PanelHbmElvssDimSupport>1</PanelHbmElvssDimSupport>
		<PanelHbmElvssDimCmdDelay>0</PanelHbmElvssDimCmdDelay>
		<!--ddic low voltage detect test-->
		<DdicLVDetectSupport>0</DdicLVDetectSupport>
		<!--horizontal/vertical line test-->
		<VtcLineSupport>0</VtcLineSupport>
		<HorLineSupport>0</HorLineSupport>
		<HorLineDuration>600</HorLineDuration>
		<!--avdd detect support-->
		<AvddDetSupport>0</AvddDetSupport>
		<AvddDetVbChannel>12</AvddDetVbChannel>
		<AvddDetLowThreshlod>0</AvddDetLowThreshlod>
		<AvddDetHighThreshlod>3000</AvddDetHighThreshlod>
		<!--pcd errflag check support-->
		<GpioPcdErrflag>0</GpioPcdErrflag>
		<PanelPcdErrflagCheckSupport>0</PanelPcdErrflagCheckSupport>
		<!--pcd cmd check support-->
		<PcdCmdsSupport>0</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		2: mask 7th bit  == ok , != NG-->
		<PcdCmdsSupportCompareMode>1</PcdCmdsSupportCompareMode>
		<!--oneside driver mode detect test-->
		<OnesideModeSupport>0</OnesideModeSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>0</ColorAodDetectSupport>
		<!--elvdd detect-->
		<PanelElvddDetectSupport>0</PanelElvddDetectSupport>
		<!--elvdd detect type-->
		<ElvddDetectType>2</ElvddDetectType>
		<ElvddDetectGpio>78</ElvddDetectGpio>
		<ElvddDetectValue>1</ElvddDetectValue>
		<ElvddDelay>20</ElvddDelay>
	</PanelEntry>
</hwlcd>
