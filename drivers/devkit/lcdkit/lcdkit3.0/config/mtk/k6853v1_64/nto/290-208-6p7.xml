<hwlcd>
	<PanelEntry>
		<!--Ldi config start-->
		<!--HBP setting-->
		<HBackPorch>26</HBackPorch>
		<!--HFP setting-->
		<HFrontPorch>73</HFrontPorch>
		<!--HS setting-->
		<HPulseWidth>6</HPulseWidth>
