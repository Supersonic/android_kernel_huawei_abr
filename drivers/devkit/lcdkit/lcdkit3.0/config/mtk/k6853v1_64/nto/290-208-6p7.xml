		<!--VS setting-->
		<VPulseWidth>2</VPulseWidth>
		<!--LP VFP setting-->
		<VFrontPorchForLP>2540</VFrontPorchForLP>
		<!--VReFresh setting-->
		<VReFresh>60</VReFresh>
		<!--Fps fuction-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<DoesLcdPowerOnTp>0</DoesLcdPowerOnTp>
		<!--DSC Params-->
		<DSCEnable>1</DSCEnable>
		<DSCVer>17</DSCVer>
		<DSCSliceMode>1</DSCSliceMode>
		<DSCRgbSwap>0</DSCRgbSwap>
