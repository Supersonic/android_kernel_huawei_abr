&bolero {
	wcd938x-codec {
		qcom,cdc-micbias1-mv = <2700>;
		qcom,cdc-micbias2-mv = <2700>;
		qcom,cdc-micbias3-mv = <2700>;
		qcom,cdc-micbias4-mv = <2700>;
	};
};

&tlmm {
	smartpakit_irq_pins: smartpakit_irq_pins {
		smartpakit_irq_default: smartpakit_irq_default {
			mux {
				pins = "gpio15", "gpio179";
				function = "gpio";
			};

			config {
				pins = "gpio15", "gpio179";
				bias-pull-up;
				input-enable;
			};
		};
	};

	box_id_default: box_id_default {
		mux {
			pins = "gpio195";
			function = "gpio";
		};

		config {
			pins = "gpio195";
			bias-pull-up;
			drive-strength = <2>;   /* 2 MA */
		};
	};

	box_id_sleep: box_id_sleep {
		mux {
			pins = "gpio195";
			function = "gpio";
		};

		config {
			pins = "gpio195";
			bias-pull-down;
			drive-strength = <2>;   /* 2 MA */
		};
	};
};

