#include <dt-bindings/clock/qcom,camcc-lahaina.h>

&soc {
	led_flash_triple_rear: qcom,camera-flash@4 {
		cell-index = <4>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		torch-light-num = <0 1>;
		torch-light-name = "torch-light0", "torch-light1";
		status = "ok";
	};

	led_flash_triple_rear_aux: qcom,camera-flash@5 {
		cell-index = <5>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	led_flash_triple_rear_aux2: qcom,camera-flash@6 {
		cell-index = <6>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	led_flash_triple_rear_aux3: qcom,camera-flash@8 {
		cell-index = <8>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
		pinctrl-names = "cam_active", "cam_suspend";
		pinctrl-0 = <&fcam_dvdd_active
					&mcam_avdd_active
					&mcam_avdd1_active>;
		pinctrl-1 = <&fcam_dvdd_suspend
					&mcam_avdd_suspend
					&mcam_avdd1_suspend>;
	};
};

&product_name_camera {
    product-name = "cet";
};

&pm8350b_gpios {
	mcam_avdd_active: mcam_avdd_active {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		output-high;
		power-source = <0>;
	};
	mcam_avdd_suspend: mcam_avdd_suspend {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <0>;
	};
};

&pm8350_gpios {
	scam_ponv_active: scam_ponv_active {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		output-high;
		power-source = <0>;
	};
	scam_ponv_suspend: scam_ponv_suspend {
		pins = "gpio8";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <0>;
	};
};

&pmk8350_gpios {
	mcam_avdd1_active: mcam_avdd1_active {
		pins = "gpio1";
		function = "normal";
		bias-disable;
		output-high;
		power-source = <0>;
	};
	mcam_avdd1_suspend: mcam_avdd1_suspend {
		pins = "gpio1";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <0>;
	};

	fcam_dvdd_active: fcam_dvdd_active {
		pins = "gpio2";
		function = "normal";
		bias-disable;
		output-high;
		power-source = <0>;
	};
	fcam_dvdd_suspend: fcam_dvdd_suspend {
		pins = "gpio2";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <0>;
	};
};

&tlmm {
	cam_sensor_mclk0_active: cam_sensor_mclk0_active {
		/* MCLK0 */
		mux {
			pins = "gpio100";
			function = "cam_mclk";
		};

		config {
			pins = "gpio100";
			bias-disable; /* No PULL */
			drive-strength = <6>; /* 2 MA */
		};
	};

	cam_sensor_mclk0_suspend: cam_sensor_mclk0_suspend {
		/* MCLK0 */
		mux {
			pins = "gpio100";
			function = "cam_mclk";
		};

		config {
			pins = "gpio100";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <6>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_active: cam_sensor_mclk3_active {
